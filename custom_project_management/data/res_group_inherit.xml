<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_manually_set_flag" model="ir.sequence">
            <field name="name">Manually Set Flag Sequence</field>
            <field name="code">manually.set.flag</field>
            <field name="prefix">NCR/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>

    <record id="module_category_quality" model="ir.module.category">
        <field name="name">Quality APP Access</field>
        <field name="description">Helps you handle your Quality APP.</field>
        <field name="sequence">1</field>
    </record>

    <record id="module_dashboard" model="ir.module.category">
        <field name="name">Dashboard View</field>
        <field name="description">Dashboard</field>
        <field name="sequence">2</field>
    </record>

    <record id="module_pm" model="ir.module.category">
        <field name="name">Projects Management View</field>
        <field name="description">Dashboard</field>
        <field name="sequence">3</field>
    </record>

    <record id="group_dashboard_user" model="res.groups">
        <field name="name">Group Dashboard</field>
        <field name="category_id" ref="custom_project_management.module_dashboard"/>
    </record>

    <record id="group_pm_user" model="res.groups">
        <field name="name">Project Management</field>
        <field name="category_id" ref="custom_project_management.module_pm"/>
    </record>

    <record id="group_custom_admin_user" model="res.groups">
        <field name="name">Custom Admin</field>
        <field name="category_id" ref="custom_project_management.module_category_quality"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>



    <record id="group_quality_maker" model="res.groups">
        <field name="name">Maker</field>
        <field name="category_id" ref="custom_project_management.module_category_quality"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_quality_checker" model="res.groups">
        <field name="name">Checker</field>
        <field name="category_id" ref="custom_project_management.module_category_quality"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_quality_approver" model="res.groups">
        <field name="name">Approver</field>
        <field name="category_id" ref="custom_project_management.module_category_quality"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record id="group_delete_activity" model="res.groups">
        <field name="name">Delete Activity</field>
        <field name="category_id" ref="custom_project_management.module_category_quality"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>



</odoo>